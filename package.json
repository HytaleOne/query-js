{
  "name": "@hytaleone/query",
  "version": "1.0.1",
  "description": "TypeScript/JavaScript client for querying Hytale servers. Get server status, player count, player list, and plugin information via UDP protocol.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hytale",
    "hytale-server",
    "query",
    "server-query",
    "server-status",
    "server-list",
    "udp",
    "status",
    "ping",
    "player-list",
    "game-server",
    "typescript",
    "nodejs",
    "motd"
  ],
  "author": "HytaleOne",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HytaleOne/query-js"
  },
  "homepage": "https://hytale.one",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
